{
  "name": "svelte-reactor",
  "version": "0.1.0",
  "description": "Powerful reactive state management for Svelte 5 with undo/redo, middleware, persistence, DevTools, and state utilities. Built on Svelte 5 Runes with full TypeScript support.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "svelte": "./dist/plugins/index.js",
      "default": "./dist/plugins/index.js"
    },
    "./devtools": {
      "types": "./dist/devtools/devtools.d.ts",
      "svelte": "./dist/devtools/devtools.js",
      "default": "./dist/devtools/devtools.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "svelte": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "bench": "vitest bench --run",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "check": "pnpm typecheck && pnpm test",
    "prepublishOnly": "pnpm clean && pnpm check && pnpm build"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "svelte-5",
    "runes",
    "state-management",
    "reactive",
    "undo",
    "redo",
    "middleware",
    "devtools",
    "persistence",
    "time-travel",
    "sveltekit",
    "ssr",
    "typescript"
  ],
  "author": "Svelte Reactor Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p1kabu/svelte-reactor.git",
    "directory": "packages/reactor"
  },
  "bugs": {
    "url": "https://github.com/p1kabu/svelte-reactor/issues"
  },
  "homepage": "https://github.com/p1kabu/svelte-reactor#readme",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^24.0.0",
    "svelte": "^5.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
